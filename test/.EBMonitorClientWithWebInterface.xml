<?xml version='1.0'?>
<xc:Partition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" xmlns:xc="http://xdaq.web.cern.ch/xdaq/xsd/2004/XMLConfiguration-30">

  <i2o:protocol xmlns:i2o="http://xdaq.web.cern.ch/xdaq/xsd/2004/I2OConfiguration-30">
    i2o:target class="EBMonitorClientWithWebInterface" instance="0" tid="95"/>
  </i2o:protocol>

  <xc:Context url="http://.lhost:.lport">

    <xc:Application class="EBMonitorClientWithWebInterface" id="15" instance="0" network="local">
      <properties xmlns="urn:xdaq-application:EBMonitorClientWithWebInterface" xsi:type="soapenc:Struct">
        <serverHost xsi:type="xsd:string">.rhost</serverHost>
        <serverPort xsi:type="xsd:unsignedLong">.rport</serverPort>
        <actAsServer xsi:type="xsd:boolean">false</actAsServer>
        <reconnectDelaySecs xsi:type="xsd:unsignedLong">5</reconnectDelaySecs>
      </properties>
    </xc:Application>
    <xc:Module>${XDAQ_ROOT}/lib/linux/x86/libpttcp.so</xc:Module>
    <xc:Module>.libpath1</xc:Module>

  </xc:Context>
</xc:Partition>
